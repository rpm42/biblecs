<script type="text/ng-template" id="chapters-view">
  <section class="chapters-section">
    <h2>{{name}}</h2>
    <br>
    <a class="back" href="/"><i class="icon-up"></i></a>
    <br>
    <ul class="chapters-list">
      <li ng-repeat="chapter in chapters">
        <a href="/{{bookId}}/{{chapter}}">{{chapter}}</a>
      </li>
    </ul>
  </section>
</script>